          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)",
              "align-items": "center",
              "display": "flex"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& when (@overlay)": {
                ".mbr-overlay": {
                  "background": "@overlayColor",
                  "opacity": "@overlayOpacity"
                }
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when not (@showIcons)": {
              ".gallery-img-wrap:after": {
                "display": "none"
              }
            },
            ".mbr-row": {
              "flex-wrap": "nowrap",
              "& when (@blockDirection)": {
                "& when (@reverseContent)": {
                  "flex-direction": "column-reverse",
                  ".image-wrap": {
                    "padding-bottom": "2rem"
                  }
                },
                ".image-wrap": {
                  "max-width": "100%",
                  "max-height": "100%",
                  "min-width": "100%",
                  "min-height": "100%"
                }
              },
              "@media (max-width: 992px)": {
                "& when (@reverseContent)": {
                  "flex-direction": "column-reverse"
                },
                ".image-wrap": {
                  "padding-bottom": "2rem"
                }
              }
            },
            "& when not (@blockDirection)": {
              ".title-block": {
                "margin": "auto",
                "width": "100%"
              },
              ".title-wrap": {
                "@media (max-width: 992px)": {
                  "margin-bottom": "2rem"
                }
              }
            },
            ".image-block, .title-block": {
              "padding-left": "0",
              "padding-right": "0"
            },
            "@media (min-width: 1400px)": {
              ".mbr-row > *": {
                "padding-left": "2rem",
                "padding-right": "2rem"
              },
              ".mbr-row": {
                "margin-left": "-2rem",
                "margin-right": "-2rem"
              }
            },
            ".link-wrap": {
              "align-items": "center",
              "@media (max-width: 767px)": {
                "justify-content": "center"
              }
            },
            ".line": {
              "width": "46px",
              "margin-right": "1rem",
              "height": "1px",
              "background": "@lineColor"
            },
            ".icon": {
              "position": "absolute",
              "top": "50%",
              "left": "50%",
              "transform": "translate(-50%, -50%)",
              "z-index": "10",
              "opacity": "0",
              "transition": "all 0.3s",
              "width": "100%",
              "text-align": "center"
            },
            "@media (max-width: 576px)": {
              ".icon": {
                "opacity": "1"
              },
              ".gallery-img-wrap:after": {
                "opacity": "0.5!important"
              }
            },
            ".gallery-img-wrap:after": {
              "background": "@hoverColor"
            },
            ".iconfont-wrapper": {
              "width": "calc(1rem ~\"+ @{imgIconsSize}rem\")",
              "height": "calc(1rem ~\"+ @{imgIconsSize}rem\")",
              "background-color": "transparent",
              "display": "inline-flex",
              "justify-content": "center",
              "align-items": "center"
            },
            ".amp-iconfont": {
              "font-size": "~\"@{imgIconsSize}rem\"",
              "width": "~\"@{imgIconsSize}rem\"",
              "color": "@imgIconsColor"
            },
            ".image-wrap": {
              "&:hover": {
                ".gallery-img-wrap:after": {
                  "opacity": "0.5!important"
                },
                ".icon": {
                  "opacity": "1"
                }
              }
            },
            ".text": {
              "text-align": "left"
            }
